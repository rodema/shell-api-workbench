{
  "extends": "../../cs-automator/tslint.json",
  "rulesDirectory": [
    "node_modules/codelyzer"
  ],
  "rules": {
    "align": [
      true,
      "parameters",
      "arguments",
      "statements"
    ],
    "ban": false,
    "deprecation": {
      "severity": "warning"
    },
    "no-unused-expression": true
  }
}