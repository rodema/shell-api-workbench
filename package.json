{
  "name": "shell-api-tester",
  "version": "1.1.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --disable-host-check",
    "start:local": "ng serve --disable-host-check --configuration start-local",
    "prebuild": "node ./scripts/createVersionInfo",
    "build": "ng build",
    "test": "node --max-old-space-size=8192 node_modules/@angular/cli/bin/ng test",
    "test:ci": "npm test -- --watch=false --code-coverage --karma-config ./karma.conf.js",
    "test:ci:headless": "SET_HEADLESS_CHROME=1 npm run test:ci",
    "lint": "ng lint",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "e2e-local": "concurrently \"npm run start\" \"wait-on http-get://localhost:4200 && npm run cy:run\" --kill-others --success first",
    "copyCert": "copy ./cert/* ./dist/ShellApiTester"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.8",
    "@angular/cdk": "^14.2.6",
    "@angular/common": "^14.2.8",
    "@angular/compiler": "^14.2.8",
    "@angular/core": "^14.2.8",
    "@angular/forms": "^14.2.8",
    "@angular/platform-browser": "^14.2.8",
    "@angular/platform-browser-dynamic": "^14.2.8",
    "@angular/router": "^14.2.8",
    "ajv": "^6.12.4",
    "fsm-shell": "^1.16.0",
    "rxjs": "~6.4.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1",
    "@fundamental-ngx/core": "0.31.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.7",
    "@angular/cli": "^14.2.7",
    "@angular/compiler-cli": "^14.2.8",
    "@angular/language-service": "^14.2.8",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "^2.0.10",
    "@types/node": "~12.11.1",
    "angular-cli-ghpages": "^1.0.5",
    "codelyzer": "^6.0.0",
    "cypress": "^8.5.0",
    "cypress-iframe": "^1.0.1",
    "cypress-multi-reporters": "^1.5.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.7.0",
    "mocha": "^9.1.2",
    "mocha-junit-reporter": "^2.0.0",
    "mochawesome": "^6.3.0",
    "mochawesome-merge": "^4.2.0",
    "mochawesome-report-generator": "^5.2.0",
    "protractor": "~5.4.0",
    "puppeteer": "^10.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.3",
    "typescript": "~4.8.4"
  }
}
